{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: webapi",
            "type": "debugpy",
            "request": "launch",
            "program": "src/web_api.py",
            "args": [
                "--config=config/web_processor.yml"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: web_front_new_sl",
            "type": "debugpy",
            "request": "launch",
            "program": "src/web_front_new_sl.py",
            "args": [
                "--config=config/web_processor.yml"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Streamlit: web_front_sl",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "src/web_front_sl.py",
                "--server.port",
                "8501",
                "--",
                "--config=config/web_processor.yml",
                "--gw_port=14440"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Streamlit Debug: web_front_sl",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "src/web_front_sl.py",
                "--server.port",
                "8501",
                "--server.headless",
                "true",
                "--",
                "--config=config/web_processor.yml",
                "--gw_port=14440"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}